prototype(Futape.NodeTypes:Blockquote) < prototype(Neos.Fusion:Value) {
	value = Futape.Bootstrap:Blockquote {
		footer {
			content = ${q(node).property('footer')}

			@process.editable = Neos.Neos:ContentElementEditable {
				property = 'footer'
			}
		}

		content = Neos.Neos:ContentCollection {
			nodePath = 'main'
			tagName = 'div'
		}
	}
}
