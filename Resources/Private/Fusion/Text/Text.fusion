prototype(Futape.NodeTypes:Text) < prototype(Futape.Bootstrap:Text) {
	style = ${q(node).property('style')}

	content = Neos.Fusion:Tag {
    	tagName = 'div'
    	content = ${q(node).property('text')}

    	@process.editable = Neos.Neos:ContentElementEditable {
    		property = 'text'
    	}
    }
}
